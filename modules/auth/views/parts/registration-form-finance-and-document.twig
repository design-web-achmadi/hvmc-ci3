            {{ lang('bank_account') }}
        </label>
        <div class="col-md-12">
            <select class="form-control populate" name="bank_account"
                    data-plugin-selectTwo data-plugin-options='{"width": "style", "placeholder": "{{ lang('select_placeholder') ~lang('bank_account') }}", "allowClear": true, "selected_id": "{{ set_value('bank_account',bank_account) }}", "selected_text": "{{ set_value('m_bank_name', bank_account) }}"}'
                    data-remote='{"url": "{{ base_url("select/master/index/bank") }}", "transformResponse": {"id": "m_bank_code", "text": "m_bank_name"} }'
                   
            >
                <option></option>
            </select>
        </div>
    </div>
