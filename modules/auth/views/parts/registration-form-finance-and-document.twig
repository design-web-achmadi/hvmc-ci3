
<div class="col-md-12 mobile-fullscreen tab-pane" id="form-finance">
    <div class="form-group row border-bottom">
        <div class="col-md-12 ">
            <h3><bold>{{ lang('header_finance') }}</bold></h3>
            
        </div>
    </div>
    <div class="form-group row">
        <label class="control-label col-md-12">
            {{ lang('header_emoney') }}
        </label>
        <div class="checkbox col-md-12">
            <label>
                <input type="checkbox" value="1" name="is_request_qris" class="is-request-qris" {{ is_request_qris == 1 ? 'checked' : '' }}>
                Single E-Money (QRIS)
            </label>
        </div>
        <div class="checkbox col-md-12">
            <label>
                <input type="checkbox" value="1" name="is_request_tpoin" class="is-request-tpoin" {{ is_request_tpoin == 1 ? 'checked' : '' }}>
                Telkomsel Money
            </label>
        </div>
        <div class="checkbox col-md-12">
            <label>
                <input type="checkbox" value="1" name="is_request_edcbca" class="is-request-edcbca" {{ is_request_edcbca == 1 ? 'checked' : '' }}>
                EDC BCA
            </label>
        </div>
    </div>
    <div class="form-group row border-bottom">
        <div class="col-md-12 ">
            <h3><bold>{{ lang('header_document') }}</bold></h3>
        </div>
    </div>
