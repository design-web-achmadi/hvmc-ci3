                    <input type="file" class="file-photo" name="image_outlet" accept="image/png, image/jpg, image/jpeg" data-placeholder="{{ lang('file_placeholder') ~ lang('photo_of_owner_npwp') }}">
                    <input type="hidden" name="photo_of_owner_npwp" value="{{ set_value('photo_of_owner_npwp', photo_of_owner_npwp) }}">
                    <input type="hidden" name="bucket_proof_payment" value="{{ set_value('bucket_proof_payment', (outlet_photo is empty ? 'TMP' : 'APP')) }}" />
                </span>
                {% if form_error('photo_of_owner_npwp') %}
                    <label class="error" for="photo_of_owner_npwp">{{form_error('photo_of_owner_npwp')}}</label>
                {% endif %}
                <label class="help-block text-muted">{{ lang('text_helper_upload_file') }}</label>
            </div>
        </div>
    </div>
